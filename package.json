{
  "name": "coursehunters-video-downloader",
  "description": "A useful tool to help you download thousands of tutorial from coursehunters.net",
  "author": "Dong Qian",
  "version": "0.0.2",
  "license": "MIT",
  "keywords": [
    "react",
    "create React App",
    "course hunters",
    "downloader",
    "download",
    "video",
    "electron",
    "tutorial",
    "programming"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:qiandongyq/coursehunters-video-downloader.git"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "concurrently": "^4.1.0",
    "electron-is-dev": "^1.0.1",
    "lodash": "^4.17.11",
    "react": "^16.8.0-alpha.1",
    "react-dom": "^16.8.0-alpha.1",
    "react-lottie": "^1.2.3",
    "react-perfect-scrollbar": "^1.4.4",
    "react-pose": "^4.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "react-sweet-progress": "^1.1.2",
    "request": "^2.88.0",
    "request-progress": "^3.0.0",
    "styled-components": "^4.1.3",
    "styled-reset": "^1.6.8",
    "validator": "^10.11.0",
    "video-react": "^0.13.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "yarn clean && yarn build && build -mwl",
    "electron-release-all": "yarn clean && yarn build && build -mwl",
    "electron-release-win": "yarn clean && yarn build && build --win",
    "electron-release-mac": "yarn clean && yarn build && build --mac",
    "electron-release-linux": "yarn clean && yarn build && build --linux",
    "postinstall": "install-app-deps package.json",
    "clean": "rm -rf build dist release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "cross-env": "^5.2.0",
    "electron": "^4.0.0",
    "electron-builder": "^20.38.4",
    "husky": "^1.3.1",
    "pretty-quick": "^1.10.0",
    "wait-on": "^3.2.0"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "appId": "com.coursehunters-video-downloader.app",
    "copyright": "Copyright Â© 2018 ${author}",
    "productName": "Coursehunters Video Downloader",
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "publish": {
      "provider": "github",
      "owner": "coursehunters-video-downloader",
      "repo": "coursehunters-video-downloader",
      "private": false
    }
  }
}
